        var allcheck = true;
        $("#all_click_btn").on("click", function(){
            allcheck = !allcheck;
            $.each($("input[type='checkbox']"), function(i, v){
                if(!allcheck){
                    $(this).prop("checked", true);
                }else{
                    $(this).prop("checked", false);
                }
            });
        });
    });


    function user_init(){
        $.ajax({
            url:"/mysql/user",
            data:{
                xml: "select_admin_list",
                sdate: sdate,
                edate: edate,
                level: $("select[name='user_level']").val(),
                appo: userAppo,
                name: userName,
                page: userPage,
                pageSize: userPageSize
            }, success: function(res){
                if(res.length > 0){
                    userTotal = res[0].TOTAL;
                var item = "";
                paging(userTotal, userPageSize, userSelectPage);
                for(var i=0; i<res.length; i++){
                    var appoUser = "<p class='table_td_txt table_td_txt_none'>미사용</p>";
                    if(res[i].ADMIN_USE == "1"){
                        appoUser = "<p class='table_td_txt table_td_use'>사용</p>";
                    }
                    var edate = res[i].ADMIN_EDATE;
                    if(edate.length==8){
                        edate = edate.substring(0,4)+"-"+edate.substring(4,6)+"-"+edate.substring(6,8);
                    }
                    item += "<tr data-userKey='"+res[i].ADMIN_KEY+"'>"
                          + "<td class='table_td td_ck'>"
                          + "<p class='table_td_txt'>"+res[i].CODE_CONTENT+"</p>"
                          + "</td>"
                          + "<td class='table_td td_ck'>"
                          + "<p class='table_td_txt'>"+res[i].ADMIN_ID+"</p>"
                          + "</td>"
                          + "<td class='table_td td_ck'>"
                          + "<p class='table_td_txt'>"+res[i].ADMIN_NAME+"</p>"
                          + "</td>"
                          + "<td class='table_td td_ck'>"
                          + "<p class='table_td_txt'>"+res[i].ADMIN_ADDR1+" "+res[i].ADMIN_ADDR2+"</p>"
                          + "</td>"
                          + "<td class='table_td td_ck'>"
                          + "<p class='table_td_txt'>"+res[i].ADMIN_FDATE+"</p>"
                          + "</td>"
                          + "<td class='table_td td_ck'>"
                          + "<p class='table_td_txt'>"+edate+"</p>"
                          + "</td>"
                          + "<td class='table_td td_ck'>"
                          + appoUser
                          + "</td>"
                          + "</tr>";
                    }
                    $("#user_table tbody").empty();
                    $("#user_table tbody").append(item);
                }else{
                    $("#user_table tbody").empty();
                }
            }
        });
    }
</script>
