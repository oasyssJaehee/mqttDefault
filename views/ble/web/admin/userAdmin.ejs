    $(window).ready(function() {
        
        user_init();

        $("#imgBtn").on("click", function(){
            // $(".chat_pop_wrap").css("display", "block");
            $("input[name='addFile']").click();
        });
        $("input[name='addFile']").on("change", function(e){
            addFiles(e, this);
        });
        
        var myCal = bulmaCalendar.attach('.date_default_inp', {
            isRange: true,
            type: 'date',
            showFooter: false,
            dateFormat: 'yyyy-MM-dd'
        });
        var myCal2 = bulmaCalendar.attach('.pop_user_edate', {
            isRange: false,
            type: 'date',
            showFooter: false,
            dateFormat: 'yyyy-MM-dd',
            minDate:curDate("-"),
            
        });
        var element = document.querySelector('.pop_user_edate');
        if (element) {
            element.bulmaCalendar.on('select', function(datepicker) {
                $(element).val(datepicker.data.value());
            });
        }
        $(".search_icon").on("click" ,function(){
            var dateInput = $("input[name='user_date']").val();
			dateInput = dateInput.split(" ~ ");
			sdate = dateInput[0];
			edate = dateInput[1];
            userName = $("input[name='user_name_inp']").val();
            if(userName == undefined){
                userName = "";
            }
            userAppo = $("select[name='user_use']").val();
            user_init();
        });
        
        $(document).on("click", "#user_table .td_ck", function(){
            popReset();
            $(".popbox_back").css("display", "block");
            adminKey = $(this).parents("tr").attr("data-userKey");
            popUserInfo();
        });
        $(".pop_cancel_btn, .pop_btn_cancel").on("click", function(){
            $(".popbox_back").css("display", "none");
            popReset();
            adminKey = "";
        });
        $("#pwResetBtn").on("click", function(){
            Confirm("비밀번호를 초기화 하시겠습니까?\n비밀번호는 [123456]으로 초기화 됩니다.", function(){
                $.ajax({
                    url:"/mysql/user",
                    data:{
                        xml: "admin_pw_reset",
                        pass:"123456",
                        adminKey: adminKey,
                        userId: "<%=session.id%>"
                    },success:function(res){
                        if(res != null){
                            Alert("초기화 되었습니다.");
                        }
                    }
                });
            });
        });
        $('body').on("change","[name='pop_user_level']",function(){
            console.log($(this).val());
            if($(this).val() == "0010003"){
                $("input[name='pop_user_edate']").closest('.block_inp_box').hide();
                $("[data-tab='pop_addr']").hide();
            }else{
                $("input[name='pop_user_edate']").closest('.block_inp_box').show();
                $("[data-tab='pop_addr']").show();
            }
        });
        $("#pop_user_save_btn").on("click", function(){
            var xml = "";
            if($("input[name='pop_user_name']").val() == ""){Alert("이름을 입력해 주세요.",function(){$("input[name='pop_user_name']").focus();});return;}
            if($("input[name='pop_user_tel']").val() == ""){Alert("전화번호를 입력해 주세요.",function(){$("input[name='pop_user_tel']").focus();});return;}
            if($("[name='pop_user_level']").val() == "0010003"){
                $("input[name='pop_user_edate']").val('');
            }else{
                if($("input[name='pop_user_edate']").val() == ""){Alert("유효기간을 입력해 주세요.",function(){$("input[name='pop_user_edate']").focus();});return;}
            }
            if($("input[name='pop_user_id']").val() == ""){Alert("아이디를 입력해 주세요.",function(){$("input[name='pop_user_id']").focus();});return;}
            if($("input[name='pop_user_addr1']").val() == ""){Alert("주소를 입력해 주세요.",function(){});return;}
            if($("input[name='pop_user_addr2']").val() == ""){Alert("상세주소를 입력해 주세요.",function(){});return;}
            if(adminKey == ""){
                xml = "admin_user_insert";
                
                $.ajax({
                    url : '/mysql/user',
                    data : {
                        xml : 'select_find_id',
                        id : $("input[name='pop_user_id']").val()
                    },
                    success : function(res){
                        var idCnt = res.length;
                        if(idCnt > 0){
                            Alert("이미 사용중인 ID입니다.",function(){
                                $("input[name='pop_user_id']");
                                return;
                            });
                        } else {
                            $.ajax({
                                url : "/mysql/user",
                                data: {
                                    xml: xml,
                                    id: $("input[name='pop_user_id']").val(),
                                    userId: "<%=session.id%>",
                                        adminKey: adminKey,
                                        name: $("input[name='pop_user_name'").val(),
                                        tel: $("input[name='pop_user_tel']").val(),
                                        addr1: $("input[name='pop_user_addr1']").val(),
                                        addr2: $("input[name='pop_user_addr2']").val(),
                                        imgname: $("input[name='user_img_name']").val(),
                                        imgpath: $("input[name='user_img_path']").val(),
                                        pass: "123456",
                                        level : $("select[name='pop_user_level']").val(),
                                        use : $("select[name='pop_user_use']").val(),
                                        hotelCode: "<%=hotelCode%>",
                                        edate:replaceAll($("input[name='pop_user_edate']").val(),"-",""),
                                } ,success: function(re){
                                    if(re != null){
                                        Alert("저장 되었습니다.", function(){
                                            window.location.reload(true);
                                        });
                                    }
                                }
                            });
                        }
                    },
                    error : function(e){
                        console.log(e.responseText);
                    }
                });
            }else{
                xml = "admin_user_update";
                $.ajax({
                url : "/mysql/user",
                data: {
                    xml: xml,
                    id: $("input[name='pop_user_id']").val(),
                    userId: "<%=session.id%>",
                        adminKey: adminKey,
                        name: $("input[name='pop_user_name'").val(),
                        tel: $("input[name='pop_user_tel']").val(),
                        addr1: $("input[name='pop_user_addr1']").val(),
                        addr2: $("input[name='pop_user_addr2']").val(),
                        imgname: $("input[name='user_img_name']").val(),
                        imgpath: $("input[name='user_img_path']").val(),
                        pass: "123456",
                        level : $("select[name='pop_user_level']").val(),
                        use : $("select[name='pop_user_use']").val(),
                        hotelCode: "<%=hotelCode%>",
                        edate:replaceAll($("input[name='pop_user_edate']").val(),"-",""),
                } ,success: function(re){
                    if(re != null){
                        Alert("저장 되었습니다.", function(){
                            window.location.reload(true);
                        });
                    }
                    }  
                });
            }
            
            
        });
